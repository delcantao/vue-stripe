{
  "name": "vue-stripe-next",
  "version": "0.1.0",
  "private": true,
  "main": "dist/vue-stripe-next.common.js",
  "module": "dist/vue-stripe-next.umd.min.js",
  "scripts": {
    "serve": "concurrently \"yarn serve:client\" \"yarn serve:server\"",
    "serve:client": "vue-cli-service serve",
    "serve:server": "nodemon src/server/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix",
    "build": "npm run build:lib",
    "build:lib": "vue-cli-service build --target lib --name vue-stripe-next src/vue-stripe/index.js"
  },
  "files": [
    "dist/*.{js,css}"
  ],
  "dependencies": {
    "@stripe/stripe-js": "^1.54.2",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "stripe": "^12.16.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "concurrently": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "vue-cli-plugin-tailwind": "^3.0.0"
  }
}